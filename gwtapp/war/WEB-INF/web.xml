<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

  <!-- Servlets -->

  <servlet>
    <servlet-name>superSpellService</servlet-name>
    <servlet-class>com.superbrown.superSpell.gwtApp.server.SuperSpellServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>superSpellService</servlet-name>
    <url-pattern>/gwtapp/SuperSpellService</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
      <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
